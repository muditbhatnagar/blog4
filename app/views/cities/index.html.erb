
<h1>Listing Cities</h1>
 <%= link_to 'New City', new_city_path %>
<table>
  <tr>
    <th>City_name</th>
  </tr>
 
  <% @city.each do |city| %>
    <tr>
      <td><%= city.city_name %></td>
      <td><%= link_to 'Edit', edit_city_path(city) %></td>
      <td><%= link_to 'Destroy', city_path(city),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td
    </tr>
  <% end %>
</table>