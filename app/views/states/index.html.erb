
<h1>Listing States</h1>
 <%= link_to 'New State', new_state_path %>
<table>
  <tr>
    <th>State_name</th>
  </tr>
 
  <% @state.each do |state| %>
    <tr>
      <td><%= state.state_name %></td>
       <td><%= link_to 'Show', state_path(state), class: "btn btn-info" %></td>
      <td><%= link_to 'Edit', edit_state_path(state), class: "btn btn-primary" %></td>
      <td><%= link_to 'Destroy', state_path(state),
              method: :delete,
              data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td
    </tr>
  <% end %>
</table>